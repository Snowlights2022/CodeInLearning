var MENU_OPTION_CURUSER                      = 0;
var MENU_OPTION_SYSUSER                      = 1;		
var MENU_OPTION_VOICE                        = 2;
var MENU_OPTION_UI_WAN_UPSTREAM_MODE         = 3;
var MENU_OPTION_IPV6                         = 4;
var MENU_OPTION_DMS                          = 5;
var MENU_OPTION_PRODUCTTYPE                  = 6;
var MENU_OPTION_WIRELESS_NUM_ADAPTOR         = 7;
var MENU_OPTION_WIRELESS_SES                 = 8;
var MENU_OPTION_GPONWAN                      = 9;
var MENU_OPTION_TELACCOUNT					 = 10;
var MENU_OPTION_SOFTVERSION					 = 11;
var MENU_OPTION_PERMISSION					 = 12; /*added by hlb,2014.04.20*/
var MENU_OPTION_PRODUCT_TYPE				 = 13; /*added by hlb,2014.04.21*/
var MENU_OPTION_VOICE_NUM				 	 = 14; /*added by csh,2014.06.27*/
var MENU_OPTION_NUM_WL_ADPT				 	 = 15; /*added by tjh,2015.07.27*/
var MENU_OPTION_SMART_GW					 = 16; /*added by yangtai 2016/02/02*/
var MENU_OPTION_BUCPE_ABILITY				 = 17; /*yangtai add 20160620*/	
var MENU_OPTION_SDN_SUPPORT				     = 18; /*yangtai add 20160620*/	

var wlItemsCgiCmd = new Array(
 	                    'wlswitchinterface0.wl',
                           'wlswitchinterface1.wl',
                           'wlswitchinterface2.wl',
